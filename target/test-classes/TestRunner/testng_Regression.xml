<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Swaglab Regression PW Suite" thread-count="2" parallel="tests" verbose="4">
	<listeners>
		<listener class-name="com.swaglab.listners.ExtentReportListner"/>
	</listeners>
	
  <test name="Login page test">
    <classes>
      <class name="com.swaglab.tests.LoginPageTest"/>
    </classes>
  </test>
  <test name="Product page test">
    <classes>
      <class name="com.swaglab.tests.ProductPageTest"/>
    </classes>
  </test>
</suite>
